<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.HBox?>

<VBox stylesheets="@parking.css" 
      alignment="CENTER" spacing="20.0"
      xmlns="http://javafx.com/javafx/8.0.171"
      xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="com.kelompok5.ipark.AllTableController">
   <children>
    <Label fx:id="secondarylabel" text="SEMUA PARKIRAN"/>

<HBox alignment="CENTER">
    <VBox alignment="TOP_LEFT" spacing="10">
<Button text="TAMBAHKAN" styleClass="add-button" prefWidth="150" prefHeight="20"/>
        <TableView fx:id="allTable" prefWidth="1050" maxWidth="1050">
            <columns>
                <TableColumn fx:id="colPlate" text="Opsi" prefWidth="50" />
               <TableColumn fx:id="colPlate" text="Nama Lokasi" prefWidth="100" />
                <TableColumn fx:id="colPlate" text="Tarif Lokasi" prefWidth="100" />
                <TableColumn fx:id="colPlate" text="Tarif Lokasi" prefWidth="100" />
                <TableColumn fx:id="colType" text="Tarif Mobil" prefWidth="100" />
                <TableColumn fx:id="colPlate" text="Tarif Motor" prefWidth="100" />
                <TableColumn fx:id="colPlate" text="Tarif Sepeda" prefWidth="100" />
                <TableColumn fx:id="colPlate" text="Kap.Mobil / Terisi" prefWidth="100" />
                <TableColumn fx:id="colType" text="Kap.Motor / Terisi" prefWidth="100" />
                <TableColumn fx:id="colPlate" text="Kap.Sepeda / Terisi" prefWidth="100" />
                <TableColumn fx:id="colPlate" text="Status" prefWidth="100" />
            </columns>
        </TableView>
    </VBox>
</HBox>


    <Button text="Keluar" prefWidth="300" prefHeight="45" onAction="#backToMain"/>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>

<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.control.ScrollPane?>

<VBox stylesheets="@style_parklocs.css" 
      alignment="CENTER" spacing="20.0"
      xmlns="http://javafx.com/javafx/8.0.171"
      xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="com.kelompok5.ipark.ParkLocsController">
   <children>
   <Label fx:id="secondarylabel" text="TABEL JENIS KENDARAAN"/>

   <ScrollPane style="-fx-background: transparent;" fitToWidth="true" prefHeight="600" prefWidth="400">
      <GridPane fx:id="grid" hgap="60" vgap="1" alignment="CENTER" style="-fx-background: transparent;" />
   </ScrollPane>

   <Button text="Keluar" prefWidth="300" prefHeight="45" onAction="#backToMain"/>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>

<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.HBox?>

<VBox stylesheets="@vehicles.css" 
      alignment="CENTER" spacing="20.0"
      xmlns="http://javafx.com/javafx/8.0.171"
      xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="com.kelompok5.ipark.VehiclesController">
   <children>
    <Label fx:id="secondarylabel" text="ATUR JENIS KENDARAAN"/>

<HBox alignment="CENTER">
    <VBox alignment="TOP_LEFT" spacing="10">
<Button text="TAMBAHKAN" styleClass="add-button" prefWidth="150" prefHeight="20"/>
        <TableView fx:id="vehicleTable" prefWidth="900" maxWidth="900">
            <columns>
                <TableColumn fx:id="colPlate" text="Opsi" prefWidth="50" />
                <TableColumn fx:id="colPlate" text="Nama" prefWidth="425" />
                <TableColumn fx:id="colType" text="Tipe Kendaraan" prefWidth="425" />
            </columns>
        </TableView>
    </VBox>
</HBox>


    <Button text="Keluar" prefWidth="300" prefHeight="45" onAction="#backToMain"/>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
